{
  "name": "@ng-nest-postgre/luv-coffee-fe",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ng": "ng",
    "serve:local": "ng serve --hmr",
    "serve:dev": "ng serve -c dev-serve",
    "serve:prod": "ng serve -c production-serve",
    "build:local": "npm run build:lib:shared && ng build",
    "build:dev": "npm run build:lib:shared && ng build -c dev",
    "build:prod": "npm run build:lib:shared && ng build -c production",
    "build:lib:shared": "ng build ngx-shared",
    "build:lib:shared:watch": "ng build ngx-shared --watch",
    "build:analyze": "npm run build:lib:shared && ng build --named-chunks --stats-json && webpack-bundle-analyzer dist/luv-coffee-fe/stats.json",
    "build:start": "npm run build:local && cd dist/luv-coffee-fe && spa-server-gzip -p 4200",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:app": "ng lint luv-coffee-fe",
    "lint:lib:shared": "ng lint ngx-shared",
    "test": "ng test --watch=false",
    "test:app": "ng test luv-coffee-fe --watch=false",
    "test:app:watch": "ng test luv-coffee-fe",
    "test:lib:shared": "ng test ngx-shared --watch=false",
    "test:lib:shared:watch": "ng test ngx-shared",
    "deploy:dev": "npm run build:dev && gcloud app deploy app.dev.yaml --project luv-coffee",
    "precommit": "npm run lint",
    "prepush": "npm run test"
  },
  "dependencies": {
    "@angular/animations": "^17.3.10",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.10",
    "@angular/compiler": "^17.3.10",
    "@angular/core": "^17.3.10",
    "@angular/forms": "^17.3.10",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.3.10",
    "@angular/platform-browser-dynamic": "^17.3.10",
    "@angular/router": "^17.3.10",
    "@angularnestpostgre/packages": "^0.0.3",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular-eslint/builder": "^17.5.1",
    "@angular-eslint/eslint-plugin": "^17.5.1",
    "@angular-eslint/eslint-plugin-template": "^17.5.1",
    "@angular-eslint/schematics": "^17.5.1",
    "@angular-eslint/template-parser": "^17.5.1",
    "@angular/cli": "^17.3.8",
    "@angular/compiler-cli": "^17.3.10",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.2.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^17.3.0",
    "typescript": "~5.4.5"
  }
}
