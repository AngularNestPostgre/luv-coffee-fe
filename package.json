{
  "name": "@ng-nest-postgre/luv-coffee-fe",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "ng": "ng",
    "serve:local": "ng serve",
    "serve:dev": "ng serve -c dev-serve",
    "serve:prod": "ng serve -c production-serve",
    "build:local": "npm run build:lib:shared && ng build",
    "build:dev": "npm run build:lib:shared && ng build -c dev",
    "build:prod": "npm run build:lib:shared && ng build -c production",
    "build:lib:shared": "ng build ngx-shared",
    "build:lib:shared:watch": "ng build ngx-shared --watch",
    "build:analyze": "npm run build:lib:shared && ng build --named-chunks --stats-json && webpack-bundle-analyzer dist/luv-coffee-fe/stats.json",
    "build:start": "npm run build:local && cd dist/luv-coffee-fe/browser && spa-server-gzip -p 4200",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:app": "ng lint luv-coffee-fe",
    "lint:lib:shared": "ng lint ngx-shared",
    "test": "ng test --watch=false",
    "test:app": "ng test luv-coffee-fe --watch=false",
    "test:app:watch": "ng test luv-coffee-fe",
    "test:lib:shared": "ng test ngx-shared --watch=false",
    "test:lib:shared:watch": "ng test ngx-shared",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "test:coverage": "ng test --no-watch --code-coverage",
    "deploy:dev": "npm run build:dev && gcloud app deploy app.dev.yaml --project luv-coffee",
    "prepare": "husky"
  },
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/cdk": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/material": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@angularnestpostgre/packages": "^0.0.8",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.1",
    "@angular-eslint/builder": "^18.0.1",
    "@angular-eslint/eslint-plugin": "^18.0.1",
    "@angular-eslint/eslint-plugin-template": "^18.0.1",
    "@angular-eslint/schematics": "^18.0.1",
    "@angular-eslint/template-parser": "^18.0.1",
    "@angular/cli": "^18.0.1",
    "@angular/compiler-cli": "^18.0.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "husky": "^9.0.11",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^18.0.0",
    "typescript": "~5.4.5"
  }
}
